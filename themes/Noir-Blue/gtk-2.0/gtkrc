style "button" {
    	text[NORMAL]		= "#54A6BB"
	fg[PRELIGHT] 			= "#00A2CC"
  	bg[NORMAL]			= "#0E0E0E"
  	bg[PRELIGHT]			= "#323232"
	engine "smooth" {
		fill {
			style = shaded
			hdirection = vertical
			vdirection = horizontal
			shade1 = 1.3
			shade2 = 0.9
		}

		focus
		{
			foreground[ACTIVE] = "#54A6BB"
			foreground[SELECTED] = "#54A6BB"
			foreground[NORMAL] = "#54A6BB"
			foreground[PRELIGHT] = "#54A6BB"
		}

		line {
			style = flat
			thickness = 1
		}

		check {
			style = clean
			motif = FALSE

			fill {
				style = shaded
				hdirection = fdiagonal
				vdirection = fdiagonal
				shade1 = 1.3
				shade2 = 0.9
			}
		}

		option {
			style = round
			motif = FALSE
			fill {
				style = shaded
				hdirection = fdiagonal
				vdirection = fdiagonal
				shade1 = 1.3
				shade2 = 0.9
			}
		}

		trough {
			fill {
				style = solid
			}
			
			line {
				style = none
			}
		}

		arrow {
			style = cleanice
			solid = TRUE
		}
	}
}

