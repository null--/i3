			foreground[ACTIVE] = "#030303"
			foreground[SELECTED] = "#030303"
			foreground[NORMAL] = "#030303"
			foreground[PRELIGHT] = "#030303"
		}


		fill {
			style = shaded
			hdirection = vertical
			vdirection = horizontal
			shade1 = 1.3
			shade2 = 0.9
		}

