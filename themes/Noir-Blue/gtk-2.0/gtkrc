#include "panel.rc"
style "default" {
	GtkWidget::interior_focus = 0
	GtkWidget::focus-line-pattern = ""
	GtkWidget::focus_padding = 0
	GtkButton::default_spacing = 2
	GtkButton::default_border = {0, 0, 0, 0}
	GtkButton::default_outside_border = {0, 0, 0, 0}

	GtkRange::trough_border = 0
	GtkRange::slider_width = 15
	GtkRange::stepper_size = 15
	GtkRange::stepper_spacing = 0

	GtkPaned::handle_size = 6
  
	GtkScrollbar::min_slider_length = 30
	GtkCheckButton::indicator_size = 13
	GtkRadioButton::indicator_size = 13
	GtkMenuBar::internal-padding = 0

	GtkButton::child_displacement_x = 0
	GtkButton::child_displacement_y = 0

	GtkMenuItem::selected_shadow_type = GTK_SHADOW_OUT

  GtkTreeView::odd_row_color = "#151515"
  #GtkTreeView::even_row_color = "#ffffff"

  GtkWidget::cursor_color           		= "#E3DFDC"
  GtkWidget::secondary_cursor_color 	= "#E3DFDC"

  GtkScrollbar::has_secondary_backward_stepper 	= 1
  GtkScrollbar::has_secondary_forward_stepper 	= 0

  GtkWidget::link-color ="#001196"
  GtkWidget::visited-link-color ="#59055F"

  	fg[NORMAL]        		= "#54A6BB"
  	fg[PRELIGHT]     	 	= "#00A2CC"
  	fg[ACTIVE]        		= "#BDBDBD"
  	fg[SELECTED]      	= "#E9E9E9"
  	fg[INSENSITIVE]   	= "#3A3A3A"

	bg[NORMAL] 		= "#030303"
	bg[PRELIGHT] 		= "#0E0E0E"
	bg[SELECTED] 		= "#323232"
	bg[ACTIVE] 			= "#252525"
	bg[INSENSITIVE] 	= "#080808"
		
	base[NORMAL] 		= "#1D1D1D"
	base[SELECTED] 	= "#0A0A0A"
	base[ACTIVE] 		= "#252525"
	base[PRELIGHT]		= "#323232"
	base[INSENSITIVE]	= "#080808"

  	text[NORMAL]      	= "#BDBDBD"
  	text[PRELIGHT]     	= "#00A2CC"
  	text[ACTIVE]      		= "#BDBDBD"
  	text[SELECTED]    	= "#54A6BB"
  	text[INSENSITIVE] 	= "#3A3A3A"

	
	engine "smooth" {
		tab_style = square

		line {
			style = flat
			thickness = 0
		}

		focus
		{
			foreground[ACTIVE] = "#54A6BB"
			foreground[SELECTED] = "#54A6BB"
			foreground[NORMAL] = "#54A6BB"
			foreground[PRELIGHT] = "#54A6BB"
		}

		grip {
			#style = mac_buds_in
			style = none
			fill {
				style = shaded
				hdirection = vertical
				vdirection = horizontal
				shade1 = 1.3
				shade2 = 0.9
			}
		}

		trough {
			fill {
				style = smooth
			}
		}

		arrow {
			style = cleanice
			solid = TRUE
		}

		check {
			style = clean
			motif = FALSE

			fill {
				style = shaded
				hdirection = fdiagonal
				vdirection = fdiagonal
				shade1 = 1.3
				shade2 = 0.9
			}
		}
	}
} widget_class "*" style "default"

