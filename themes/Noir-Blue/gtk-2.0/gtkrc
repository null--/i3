style "hscales"  {
	bg[NORMAL]		= "#323232"
	bg[PRELIGHT] 		= "#424242"
	bg[SELECTED]		= "#0E0E0E"
	
	GtkRange::trough_border = 0
    	GtkRange::slider-width 	= 16
    	GtkScale::slider-length 	= 8

	engine "smooth" {	
		line {
			style = flat
			thickness = 1
		}

		focus
		{
			foreground[ACTIVE] = "#030303"
			foreground[SELECTED] = "#030303"
			foreground[NORMAL] = "#030303"
			foreground[PRELIGHT] = "#030303"
		}


		fill {
			style = shaded
			hdirection = vertical
			vdirection = horizontal
			shade1 = 1.3
			shade2 = 0.9
		}

		trough {
			xpadding = 8
			ypadding = 1
			show_value = TRUE
			fill {
				style = solid
			}
		}

		grip {
			#style = mac_buds_in
			style = none
		}

		real_sliders = FALSE
	}
}

style "vscales"  {
	bg[NORMAL]		= "#323232"
	bg[PRELIGHT] 		= "#424242"
	bg[SELECTED]		= "#0E0E0E"

