! swap left less with page up
keycode 94  = Prior
! swap right alt with page down
clear Mod1
add Mod1 = Alt_L Alt_L
keycode 108 = Next
! swap pause with del
keycode 127 = Delete
! swap insert with home
keycode 118  = Home
! swap del with end
keycode 119  = End
! swap scrlock with insert
keycode 78   = Insert
